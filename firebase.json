{
	"hosting": {
		"site": "supercolor-cms",
		"public": "public",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"predeploy": [
			"npm run build"
		],
		"rewrites": [
			{
				"source": "**",
				"function": "cms"
			}
		]
	},
	"functions": {
		"source": "functions",
		"runtime": "nodejs16",
		"predeploy": [
			"npm --prefix \"$RESOURCE_DIR\" run build"
		]
	}
}
